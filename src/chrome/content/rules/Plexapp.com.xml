<!--
	For other Plex coverage, see Plex.tv.xml.


	Fully covered subdomains:

		- (www.)
		- dev
		- elan
		- my
		- wiki


	Mixed content:

		- Images on elan from $self *

	* Secured by us

-->
<ruleset name="Plexapp.com">

	<target host="plexapp.com" />
	<target host="*.plexapp.com" />


	<rule from="^http://((?:dev|elan|my|wiki|www)\.)?plexapp\.com/"
		to="https://$1plexapp.com/" />

</ruleset>
