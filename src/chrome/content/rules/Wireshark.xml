<!--
	Nonfunctional subdomains:

		- sharkfest *

	* Shows www


	Fully covered subdomains:

		- (www.)
		- anonsvn
		- blog

-->
<ruleset name="Wireshark (partial)">

	<target host="wireshark.org" />
	<target host="*.wireshark.org" />


	<rule from="^http://((?:anonsvn|blog|www)\.)?wireshark\.org/"
