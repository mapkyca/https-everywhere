<ruleset name="Bitcoin Foundation.org">

	<target host="bitcoinfoundation.org" />
	<target host="*.bitcoinfoundation.org" />


	<securecookie host="^(?:members)?\.bitcoinfoundation\.org$" name=".+" />


	<rule from="^http://(members\.|www\.)?bitcoinfoundation\.org/"
		to="https://$1bitcoinfoundation.org/" />

</ruleset>
