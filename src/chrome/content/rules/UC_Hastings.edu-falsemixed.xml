<!--
	For rules not causing false/broken MCB, see UC_Hastings.edu.xml.

-->
<ruleset name="UC Hastings.edu (false MCB)" platform="mixedcontent">

	<target host="journals.uchastings.edu" />
	<target host="library.uchastings.edu" />
	<target host="librarysource.uchastings.edu" />


	<rule from="^http://(journals|library|librarysource)\.uchastings\.edu/"
		to="https://$1.uchastings.edu/" />

</ruleset>
