<!--
	Problematic subdomains:

		- ssrg *

	* No trust chain


	Fully covered subdomains:

		- (www.)
		- (www.)ertos
		- owa
		- webmail

-->
<ruleset name="NICTA (partial)">

	<target host="nicta.com.au" />
	<target host="*.nicta.com.au" />


	<rule from="^http://((?:(?:www\.)?ertos|owa|webmail|www)\.)?nicta\.com\.au/"
		to="https://$1nicta.com.au/" />

</ruleset>
