<!--
	Fully covered subdomains:

		- (www.)
		- assets
		- blog
		- earhart

-->
<ruleset name="Flameeyes">

	<target host="flameeyes.eu" />
	<target host="*.flameeyes.eu" />


	<securecookie host="^blog\.flameeyes\.eu$" name=".+" />


	<rule from="^http://((?:assets|blog|earhart|www)\.)?flameeyes\.eu/"
		to="https://$1flameeyes.eu/" />

</ruleset>
