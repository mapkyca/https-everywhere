<!--
	For other Red Hat coverage, see Red_Hat.xml.


	(www.): mismatched

-->
<ruleset name="RH Cloud.com">

	<target host="rhcloud.com" />
	<target host="*.rhcloud.com" />


	<!--	Redirects like so:
					-->
	<rule from="^http://(?:www\.)?rhcloud\.com/+"
		to="https://openshift.com/" />

	<rule from="^http://([\w-]+)\.rhcloud\.com/"
		to="https://$1.rhcloud.com/" />

</ruleset>
