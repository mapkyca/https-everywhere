<!--
	^: cert only matches www

-->
<ruleset name="Affiliate-B.com">

	<target host="affiliate-b.com" />
	<target host="*.affiliate-b.com" />


	<rule from="^http://(?:www\.)?affiliate-b\.com/"
		to="https://www.affiliate-b.com/" />

	<rule from="^http://track\.affiliate-b\.com/"
		to="https://track.affiliate-b.com/" />

</ruleset>
