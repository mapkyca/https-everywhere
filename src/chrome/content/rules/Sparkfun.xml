<ruleset name="Sparkfun">

	<target host="sparkfun.com" />
	<target host="*.sparkfun.com" />


	<!--	Not secured by server:
					-->
	<!--securecookie host="^\.forum\.sparkfun\.com$" name=".+" /-->
	<securecookie host="^(?:forum)?\.sparkfun\.com$" name=".+" />


	<rule from="^http://((?:forum|static|www)\.)?sparkfun\.com/"
		to="https://$1sparkfun.com/" />

</ruleset>
