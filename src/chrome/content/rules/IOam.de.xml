<ruleset name="IOam.de">

	<target host="*.ioam.de" />


	<rule from="^http://script\.ioam\.de/"
		to="https://script.ioam.de/" />

	<!--	Redirect drops path but not args:
							-->
	<rule from="^http://www\.ioam\.de/[^?]*(?=$|\?)"
		to="https://www.infonline.de/" />

</ruleset>
