<ruleset name="HackerOne.com">

	<target host="hackerone.com" />
	<target host="*.hackerone.com" />
	<target host="*.hackerone-user-content.com" />


	<securecookie host="^\.hackerone(?:-user-content)?\.com$" name=".+" />


	<rule from="^http://(www\.)?hackerone\.com/"
		to="https://$1hackerone.com/" />

	<rule from="^http://profile-photos\.hackerone-user-content\.com/"
		to="https://profile-photos.hackerone-user-content.com/" />

</ruleset>
