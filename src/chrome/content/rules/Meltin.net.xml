<!--
	Nonfunctional subdomains:

		- martin *
		- studios *

	* Shows ozlabs.org; mismatched, CN: ozlabs.org

-->
<ruleset name="Meltin.net (partial)" platform="cacert">

	<target host="meltin.net" />
	<target host="www.meltin.net" />


	<rule from="^http://(www\.)?meltin\.net/"
		to="https://$1meltin.net/" />

</ruleset>
