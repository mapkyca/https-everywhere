-->
<ruleset name="Minds.com (Partial)">

	<target host="minds.com" />
	<target host="*.minds.com" />
	<target host="*.minds.org" />

	<rule from="^http://minds\.com/"
		to="https://minds.com/" />
	<rule from="^http://([^/:@]*)\.minds\.com/"
		to="https://$1.minds.com/" />
	<rule from="^http://([^/:@]*)\.minds\.org/"
                to="https://$1.minds.org/" />
</ruleset>
