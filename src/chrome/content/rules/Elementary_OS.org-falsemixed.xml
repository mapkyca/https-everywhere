<!--
	For rules not causing false/broken MCB, see Elementary_OS.org.xml.

-->
<ruleset name="Elementary OS.org (false MCB)" platform="mixedcontent">

	<target host="elementaryos.org" />
	<target host="*.elementaryos.org" />


	<securecookie host="^\.?elementaryos\.org$" name=".+" />


	<rule from="^http://(www\.)?elementaryos\.org/"
		to="https://$1elementaryos.org/" />

</ruleset>
