<ruleset name="Telegram.org">
  <target host="telegram.org" />
  <target host="www.telegram.org" />
  <target host="web.telegram.org" />
  <target host="core.telegram.org" />
  <rule from="^http://(www\.)?telegram\.org/" to="https://www.telegram.org/" />
  <rule from="^http://web\.telegram\.org/" to="https://web.telegram.org/" />
  <rule from="^http://core\.telegram\.org/" to="https://core.telegram.org/" />
</ruleset>
