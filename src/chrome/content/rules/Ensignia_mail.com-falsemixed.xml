<!--
	For rules not causing false/broken MCB, see Ensignia_mail.com.xml.

-->
<ruleset name="Ensignia mail.com (false MCB)" platform="mixedcontent">

	<target host="ensigniamail.com" />
	<target host="*.ensigniamail.com" />


	<rule from="^http://(www\.)?ensigniamail\.com/"
		to="https://$1ensigniamail.com/" />

</ruleset>
